{
	"manifest_version":2,

	"name":"Page Tweeter",
	"description":"authorizes access to an api",
	"version":"1.0.0",

	"permissions": [ 
		"tabs", 
		"https://api.twitter.com/*",
		"https://www.google.com/*"
	],
	"browser_action":{
		"default_icon":"icon.png"
	},
	"background": {
		"scripts": [
	    	"chrome_ex_oauthsimple.js",
	    	"chrome_ex_oauth.js",
	    	"background.js"
    	]
  	}
}